<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tourapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        > <activity android:name=".PhrasesActivity"></activity>
        <activity android:name=".ColorsActivity" />
        <activity android:name=".FamilyMembersActivity"
            android:label="@string/food"/>
        <activity android:name=".NumbersMainActivity"
